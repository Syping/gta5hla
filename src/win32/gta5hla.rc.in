#define RT_MANIFEST 24
#define CREATEPROCESS_MANIFEST_RESOURCE_ID 1
CREATEPROCESS_MANIFEST_RESOURCE_ID RT_MANIFEST "gta5hla.exe.manifest"
#include <windows.h>
VS_VERSION_INFO     VERSIONINFO
FILEVERSION         @gta5hla_VERSION_MAJOR@, @gta5hla_VERSION_MINOR@, @gta5hla_VERSION_PATCH@, 0
PRODUCTVERSION      @gta5hla_VERSION_MAJOR@, @gta5hla_VERSION_MINOR@, @gta5hla_VERSION_PATCH@, 0
FILEFLAGSMASK       0x3fL
FILEFLAGS           0
FILEOS              VOS_NT_WINDOWS32
FILETYPE            VFT_APP
FILESUBTYPE         VFT2_UNKNOWN
BEGIN
    BLOCK   "VarFileInfo"
    BEGIN
        VALUE   "Translation",  0x0809,  1200
    END
    BLOCK   "StringFileInfo"
    BEGIN
        BLOCK   "040904b0"
        BEGIN
            VALUE   "CompanyName",      "Syping"
            VALUE   "FileDescription",  "Hardlinking Assistant for GTA V"
            VALUE   "FileVersion",      "@gta5hla_VERSION@"
            VALUE   "InternalName",     "gta5hla"
            VALUE   "LegalCopyright",   "Copyright © @gta5hla_BUILD_YEAR@ Syping"
            VALUE   "OriginalFilename", "gta5hla.exe"
            VALUE   "ProductName",      "gta5hla"
            VALUE   "ProductVersion",   "@gta5hla_VERSION@"
        END
    END
END
